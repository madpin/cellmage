# Environment variables for NotebookLLM (cellmage)
# Copy this file to .env and fill in your values

# Required for most LLM interactions
CELLMAGE_API_KEY=YOUR_LLM_API_KEY
CELLMAGE_API_BASE=YOUR_LLM_API_BASE_URL # e.g., https://api.openai.com/v1

# Optional Settings (Defaults shown)
# CELLMAGE_LOG_LEVEL=INFO
# CELLMAGE_PERSONAS_DIR=llm_personas
# CELLMAGE_SNIPPETS_DIR=snippets
# CELLMAGE_CONVERSATIONS_DIR=snippets
# CELLMAGE_DEFAULT_MODEL= # e.g., gpt-4o
